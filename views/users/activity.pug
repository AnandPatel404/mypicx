extends ../layout/users
block content 
    .d-flex.align-items-center.justify-content-between.gap-2.mb-4.flex-wrap
        div 
            nav(aria-label="breadcrumb")
                ol.breadcrumb.mb-0.p-0
                    li.breadcrumb-item
                        a(href="/users/dashboard") Dashboard 
                    li.breadcrumb-item.active(aria-current="page") Profile



    .row
        .col-xl-3.col-lg-12.theiaStickySidebar
            .card.mb-3.mb-xl-0
                .card-body
                    .settings-sidebar
                        h5.mb-3.fs-17 General Settings
                        .list-group.list-group-flush.settings-sidebar
                            a.d-block.p-2.fw-medium(href="/users/profile") Profile
                            a.d-block.p-2.fw-medium(href="/users/security") Security
                            a.d-block.p-2.fw-medium.active(href="/users/activity") Activity 

        .col-xl-9.col-lg-12
            .card.mb-0
                .card-body
                    .table-responsive.custom-table
                        table.table.dataTable.table-nowrap#deals-project
                            thead.table-light
                                tr
                                    th Ip
                                    th Browser
                                    th type
                                    th Login at
                            tbody   
                                tr 
                                    td 103.248.242.2         
                                    td Firefox         
                                    td Login         
                                    td 1/24/2026, 10:39:23 AM   